{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "description": "test coverage for [html-webpack-plugin (v2.28.0)](https://github.com/ampedandwired/html-webpack-plugin) [![npm package](https://img.shields.io/npm/v/npmtest-html-webpack-plugin.svg?style=flat-square)](https://www.npmjs.org/package/npmtest-html-webpack-plugin) [![travis-ci.org build-status](https://api.travis-ci.org/npmtest/node-npmtest-html-webpack-plugin.svg)](https://travis-ci.org/npmtest/node-npmtest-html-webpack-plugin)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "html-webpack-plugin": "*",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmtest/node-npmtest-html-webpack-plugin",
    "keywords": [
        "coverage",
        "test",
        "html-webpack-plugin"
    ],
    "license": "MIT",
    "main": "lib.npmtest_html_webpack_plugin.js",
    "name": "npmtest-html-webpack-plugin",
    "nameAlias": "npmtest_html_webpack_plugin",
    "nameOriginal": "npmtest-html-webpack-plugin",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmtest/node-npmtest-html-webpack-plugin.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.2"
}
